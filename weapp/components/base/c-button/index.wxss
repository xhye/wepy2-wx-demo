.c-button {
  width: 100%;
  min-height: 98rpx;
  height: 98rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  background-color: transparent;
}
.c-button button {
  max-width: 92%;
  width: 92%;
  height: 100%;
  border-radius: 12rpx;
  font-size: 36rpx;
  line-height: 98rpx;
}

